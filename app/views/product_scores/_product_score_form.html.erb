<%= form_for product_score, data: {review_ticket_id: review_ticket.id}, remote: true do |f| %>
	<%= product_score.product.name %>
	<div class="product_score"></div>
	<%= hidden_field_tag 'review_ticket_id', review_ticket.id %>
	<%= f.submit "" %>
<% end %>


<script>
	$('.product_score').raty({
		path: '/stars/',
		scoreName: 'product_score'
	});
</script>